{
  "name": "proyecto-ensek",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "start": "node server/index.js",
    "server": "cd server && nodemon index.js",
    "client": "vite",
    "dev:full": "concurrently \"npm run server\" \"npm run client\"",
    "seed:db": "cd server && node seeder.js",
    "init:db": "cd server && node scripts/initDb.js",
    "setup": "npm install && cd server && npm install",
    "prod:setup": "npm ci && cd server && npm ci && npm run build",
    "test": "cd server && node scripts/test.js",
    "create:admin": "cd server && node scripts/createAdmin.js"
  },
  "dependencies": {
    "@emailjs/browser": "^4.4.1",
    "@fullcalendar/core": "^6.1.15",
    "@fullcalendar/daygrid": "^6.1.15",
    "@fullcalendar/interaction": "^6.1.15",
    "@fullcalendar/react": "^6.1.15",
    "@fullcalendar/timegrid": "^6.1.15",
    "axios": "^1.9.0",
    "bootstrap": "^5.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.6.0",
    "sweetalert2": "^11.22.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^9.2.1",
    "eslint": "^8.55.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "vite": "^6.3.4"
  }
}
